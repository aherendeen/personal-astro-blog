{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "personal-astro-blog",
	"type": "module",
	"version": "0.1.0",
	"description": "A minimal Astro template for personal blogs",
	"author": "Your Name",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aherendeen/personal-astro-blog.git"
	},
	"homepage": "https://github.com/aherendeen/personal-astro-blog#readme",
	"bugs": {
		"url": "https://github.com/aherendeen/personal-astro-blog/issues"
	},
	"keywords": [
		"astro",
		"template",
		"blog",
		"personal-website",
		"tailwindcss",
		"mdx",
		"typescript"
	],
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"typecheck": "astro check",
		"check": "npm run format:check && npm run lint && npm run typecheck"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/mdx": "^4.3.5",
		"@astrojs/rss": "^4.0.12",
		"@astrojs/sitemap": "^3.5.1",
		"@fontsource-variable/inter": "^5.2.7",
		"@fontsource-variable/jetbrains-mono": "^5.2.7",
		"@fontsource/silkscreen": "^5.2.7",
		"@iconify-json/mdi": "^1.2.3",
		"@tailwindcss/vite": "^4.1.13",
		"astro": "^5.13.7",
		"astro-icon": "^1.1.5",
		"dayjs": "^1.11.18",
		"mdast-util-to-string": "^4.0.0",
		"reading-time": "^1.5.0",
		"rehype-figure-title": "^1.0.0",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.35.0",
		"eslint-plugin-jsx-a11y": "^6.8.1",
		"@tailwindcss/typography": "^0.5.16",
		"@typescript-eslint/eslint-plugin": "^8.44.0",
		"@typescript-eslint/parser": "^8.44.0",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-astro": "^1.3.1",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"typescript-eslint": "^8.44.0"
	}
}
